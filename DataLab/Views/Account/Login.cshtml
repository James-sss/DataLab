@{
    ViewBag.Title = "Login";
 }


<div class="login-section">
    <div class="form-container">
        <div class="row ">
            <div class="col-md-12 col-lg-12 col-md-12 col-lg-12">
                <div class="form-div">
                    <img src="~/Images/DataLab-Logo2.png" class="logo-img img-thumbnail">
                    <p>Authorized Users Only</p>
                    <form method="post">
                        <div class="form-content">
                            <div class="form-group">
                                <input type="text" class="form-control" placeholder="Email">
                            </div>
                            <div class="form-group">
                                <input type="password" class="form-control" placeholder="Password">
                            </div>
                        </div>
                        <button class="btn btn-group-lg btn-lg">
                            Log in
                        </button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>